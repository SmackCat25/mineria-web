    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"nlp = spacy.load(\\\"es_core_news_sm\\\")\\n\",\n",
    "    \"stop_words = set(nltk.corpus.stopwords.words('spanish'))\"\n",
    "   ],\n",
